		"@sveltejs/kit": "1.15.9",
		"@typescript-eslint/eslint-plugin": "5.59.1",
		"@typescript-eslint/parser": "5.59.1",
		"concurrently": "^8.0.1",
		"eslint": "8.39.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.27.0",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.0",
		"svelte": "3.58.0",
		"svelte-check": "3.2.0",
		"tslib": "2.5.0",
		"typescript": "5.0.4",
		"vite": "4.3.3",
		"vitest": "0.30.1"
